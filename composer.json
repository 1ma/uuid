{
    "name": "uma/uuid",
    "description": "Lightweight UUID library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php-64bit": ">=7.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.13",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "UMA\\Uuid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UMA\\Uuid\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "bench": [
            "@composer install",
            "php vendor/bin/phpbench run --report=env --report=aggregate"
        ],
        "test" : [
            "@composer install",
            "php vendor/bin/phpunit"
        ]
    }
}
